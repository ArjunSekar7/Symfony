{% block stylesheets %}    
 <link href="{{ asset('css/commoncss.css') }}" rel="stylesheet"/> 
 {%endblock%}
 
        <div class="example-wrapper">
            <h1>Login</h1>
            {% for error in form.vars.errors.form.getErrors(true) %}
                <p class="error_msg">
                    *{{ error.message }}</p>
            {% endfor %}
            {{ form_start(form) }}
            <p class="error_msg">
                {{ message }}
            </p>
            {{ form_widget(form) }}
            <button class="btn">{{ button_label|default('Login') }}</button>
            {{ form_end(form) }}
            <a class="register" href={{path('register')}}>Register</a>
        </div>
