{% block stylesheets %}    
 <link href="{{ asset('css/register.css') }}" rel="stylesheet"/> 
  <link href="{{ asset('css/commoncss.css') }}" rel="stylesheet"/> 
 {%endblock%}

<div class="example-wrapper">
    <h1>Registration</h1>
        {% if message != null%}
           <h3 class="error_msg"> {{ message }} </h3>
        {% endif %}

        {% for error in form.vars.errors.form.getErrors(true) %}
             <p class="error_msg"> * {{ error.message }}</p>
       {% endfor %}
      
        {{ form_start(form) }}
            {{ form_widget(form) }}
            <p class="error_msg">* fields are required</p>
            {{ form_end(form) }}
</div>
